{
    "name": "ligif",
    "version": "0.1.1",
    "description": "tool library for gif",
    "main": "dist/ligif.umd.js",
    "module": "dist/ligif.es.js",
    "type": "module",
    "types": "typings/main.d.ts",
    "scripts": {
        "dev": "EXAMPLE=true vite",
        "build-go": "tinygo build -o src/gif.wasm -target wasm  -heap-size 200M ./main.go",
