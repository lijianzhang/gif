    "devDependencies": {
        "@gaoding/eslint-config-typescript": "^1.1.1",
        "vue": "^3.0.5",
        "@vitejs/plugin-vue": "^1.0.4",
        "@vue/compiler-sfc": "^3.0.5",
        "@vuedx/typecheck": "^0.4.1",
        "@vuedx/typescript-plugin-vue": "^0.4.1",
        "ant-design-vue": "^2.0.0-rc.9",
        "eslint": "^5.16.0",
        "typescript": "^4.1.1",
        "vite": "^2.0.0-beta.61"
    }
}
